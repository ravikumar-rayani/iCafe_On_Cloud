  </configSections>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <connectionStrings>
    <!--<add name="iCafeEntities" connectionString="Server=MYCOMPUTER\DEVSQLSERVER;initial catalog=iCafeDB;user id=sa;password=sa;MultipleActiveResultSets=True;" providerName="System.Data.SqlClient" />-->
    <!--For DB First-->
    <add name="iCafeEntities" connectionString="metadata=res://*/iCafeModel.csdl|res://*/iCafeModel.ssdl|res://*/iCafeModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=MYCOMPUTER\DEVSQLSERVER;initial catalog=iCafeDB;user id=sa;password=sa;multipleactiveresultsets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <system.web>
    <compilation targetFramework="4.5.2" />
    <httpRuntime targetFramework="4.5.2" />
  <httpHandlers>
      <add verb="*" path="routes.axd" type="AttributeRouting.Web.Logging.LogRoutesHandler, AttributeRouting.Web" />
    </httpHandlers></system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules runAllManagedModulesForAllRequests="false">
      <remove name="WebDAVModule" />
    </modules>
